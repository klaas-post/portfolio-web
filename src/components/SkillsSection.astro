---
import Skill from "./Skill.astro";

const skills: Skill[] = [
    { name: ".NET", progress: 75 },
    { name: "JavaScript", progress: 60 },
    { name: "TypeScript", progress: 70 },
    { name: "CSS", progress: 64 },
    { name: "React", progress: 65 },
    { name: "Svelte", progress: 57 },
    { name: "Astro", progress: 24 },
    { name: "Swift", progress: 45 },
    { name: "Python", progress: 76 },
    { name: "Azure", progress: 60 },
    { name: "AWS", progress: 45 },
    { name: "Firebase", progress: 80 },
]
---

<section>
    <h3>SKILLS</h3>
    <hr>
    <div>
        {skills.map((skill) => (
            <Skill name={skill.name} progress={skill.progress}/>
        ))}
    </div>
</section>