---
import Education from "./Education.astro"
const Educations: Education[] = [
    {school: "Windesheim", location: "Zwolle", course: "Bachelor of Science - HBO-ICT Software Engineer"},
    {school: "Emelwerda", location: "Emmeloord", course: "Natuur en Techniek"}
]
---

<section>
    <h3>EDUCATION</h3>
    <hr>
    <div>
        {Educations.map((education) => (
            <Education school={education.school} location={education.location} course={education.course}/>
        ))}
    </div>
</section>

